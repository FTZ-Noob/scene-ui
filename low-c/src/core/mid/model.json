{
    "AppName": "student",
    "ErrorCode": {
        "Suc": 0,
        "BadRequest": 1
    },
    "Model": {
        "$Database": "2022student",
        "User": {
            "usernick": "string",
            "pwd": "string",
            "userID": {
                "type": "string",
                "isID": true
            }
        }
    },
    "Operation": {
        "$BaseURL": [ "./", "localhost:8080/" ],
        "Router": {
            "/auth": {
                "login": {
                    "Response": {
                        "done": "boolean"
                    },
                    "Data": {
                        "username": "string",
                        "pwd": "string"
                    },
                    "ExtraData": {
                        "userID": "string"
                    }
                }
            }
        }        
    }
}