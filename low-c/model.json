{
    "AppName": "student",
    "ErrorCode": {
        "Suc": 0,
        "BadRequest": 1
    },
    "Model": {
        "$Cover": true,
        "$Database": "2022student",
        "tables": {
            "User": {
                "usernick": "string",
                "pwd": "string",
                "userID": {
                    "type": "string",
                    "isID": true
                }
            }
        }
    },
    "Operation": {
        "$BaseURL": [ "./", "localhost:8080/" ],
        "Router": {
            "/auth": {
                "login": {
                    "Response": {
                        "done": "boolean"
                    },
                    "Data": {
                        "username": "string",
                        "pwd": "string"
                    },
                    "ExtraData": {
                        "userID": "string"
                    }
                }
            }
        }        
    }
}